{
    "name": "pristavu/laravel-cancellation",
    "description": "A laravel cancellation package",
    "keywords": [
        "signifly",
        "cancellation",
        "laravel"
    ],
    "homepage": "https://github.com/pristavu/laravel-cancellation",
    "license": "MIT",
    "authors": [
        {
            "name": "Morten Poul Jensen",
            "email": "mpj@signifly.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Signifly\\Cancellation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Signifly\\Cancellation\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config":  {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Signifly\\Cancellation\\CancellationServiceProvider"
            ]
        }
    }
}
